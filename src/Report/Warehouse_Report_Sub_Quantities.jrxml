    <queryString>
		<![CDATA[-- Ready, Reserved, Total Quantities
SELECT
    SUM(Inventory.Ready_Qty) AS TTL_READY_QTY,
    SUM(Inventory.Reserved_Qty) AS TTL_RESERVED_QTY,
    SUM(Inventory.Reserved_Qty) + SUM(Inventory.Ready_Qty) AS TTL_QTY
FROM
    Inventory]]>
